{
  "name": "dungeon-tools",
  "version": "0.1.0",
  "scripts": {
    "start": "ng serve",
    "build": "npm run build:package && npm run build:routes_fix",
    "build:package": "ng build --configuration production=true --baseHref=/dungeon-tools/",
    "build:routes_fix": "cp dist/dungeon-tools/index.html dist/dungeon-tools/404.html",
    "test": "npm run test:code && npm run test:e2e",
    "test:code": "ng test",
    "test:e2e": "ng e2e",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "check": "npm run lint && npm run test && npm run build",
    "publish": "npm run build && npm run publish:clean && npm run publish:copy",
    "publish:clean": "rm -rf docs",
    "publish:copy": "cp -r dist/dungeon-tools/ docs/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.16",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "^12.2.16",
    "@angular/compiler": "^12.2.16",
    "@angular/core": "^12.2.16",
    "@angular/forms": "^12.2.16",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "^12.2.16",
    "@angular/platform-browser-dynamic": "^12.2.16",
    "@angular/router": "^12.2.16",
    "@level23archbard/storage-service": "~0.0.5",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.18",
    "@angular-eslint/builder": "2.0.2",
    "@angular-eslint/eslint-plugin": "2.0.2",
    "@angular-eslint/eslint-plugin-template": "2.0.2",
    "@angular-eslint/schematics": "2.0.2",
    "@angular-eslint/template-parser": "2.0.2",
    "@angular/cli": "^12.2.18",
    "@angular/compiler-cli": "^12.2.16",
    "@types/jasmine": "^3.6.11",
    "@types/node": "^12.20.17",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "codelyzer": "^6.0.2",
    "eslint": "^7.31.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "^4.3.5"
  }
}