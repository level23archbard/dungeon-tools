{
  "name": "dungeon-tools",
  "version": "0.2.3",
  "scripts": {
    "start": "ng serve",
    "build": "npm run build:package && npm run build:routes_fix",
    "build:package": "ng build --configuration production --base-href /dungeon-tools/",
    "build:routes_fix": "cp dist/dungeon-tools/index.html dist/dungeon-tools/404.html",
    "test": "npm run test:code && npm run test:e2e",
    "test:code": "ng test",
    "test:e2e": "ng e2e",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "check": "npm run lint && npm run test && npm run build",
    "publish": "npm run build && npm run publish:clean && npm run publish:copy",
    "publish:clean": "rm -rf docs",
    "publish:copy": "cp -r dist/dungeon-tools/ docs/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.10",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.2.10",
    "@angular/compiler": "^14.2.10",
    "@angular/core": "^14.2.10",
    "@angular/forms": "^14.2.10",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^14.2.10",
    "@angular/platform-browser-dynamic": "^14.2.10",
    "@angular/router": "^14.2.10",
    "@level23archbard/storage-service": "~0.0.6",
    "@svgdotjs/svg.js": "^3.1.2",
    "honeycomb-grid": "^4.0.1",
    "rxjs": "^7.5.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.9",
    "@angular-eslint/builder": "^14.1.2",
    "@angular-eslint/eslint-plugin": "^14.1.2",
    "@angular-eslint/eslint-plugin-template": "^14.1.2",
    "@angular-eslint/schematics": "^14.1.2",
    "@angular-eslint/template-parser": "^14.1.2",
    "@angular/cli": "^14.2.9",
    "@angular/compiler-cli": "^14.2.10",
    "@types/jasmine": "^3.6.11",
    "@types/node": "^12.20.17",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "codelyzer": "^6.0.2",
    "eslint": "^8.27.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "^4.6.4"
  }
}
